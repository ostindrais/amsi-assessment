<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <tab id="ams_assessment_tab" translate="label" sortOrder="1">
            <label>AMS Assessments</label>
        </tab>
        <section id="ams_assessment" translate="label" type="text" sortOrder="100" showInDefault="1"
                 showInWebsite="1" showInStore="1">
            <label>Enable CSV Download</label>
            <tab>ams_assessment_tab</tab>
            <resource>AmsAssessment_CartCSVDownload::config</resource>
            <group id="downloads" translate="label" type="text" sortOrder="10" showInDefault="1" showInWebsite="1"
                   showInStore="1">
                <label>Enable CSV Download</label>
                <field id="csv_download_enabled" translate="label" type="checkbox" showInDefault="1" canRestore="1"
                       showInWebsite="1" showInStore="1">
                    <label>Enabled</label>
                    <source_model>Magento\Config\Model\Config\Source\Enabledisable</source_model>
                </field>
            </group>
        </section>
    </system>
</config>
